{
  "name": "berlioz/router",
  "type": "library",
  "description": "Berlioz Router is a PHP library for manage HTTP routes, respecting PSR-7 (HTTP message interfaces) standard.",
  "minimum-stability": "stable",
  "license": "MIT",
  "homepage": "https://getberlioz.com",
  "authors": [
    {
      "name": "Ronan Giron",
      "email": "ronan@getberlioz.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Berlioz\\Router\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Berlioz\\Router\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.0",
    "berlioz/http-message": "^2.0.0",
    "psr/http-message": "^1.0",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
